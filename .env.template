# OpenClaw Configuration
DOMAIN=your-domain.com
OPENCLAW_IMAGE=openclaw:latest
OPENCLAW_GATEWAY_TOKEN=<your-secure-token-here>
OPENCLAW_GATEWAY_BIND=lan
OPENCLAW_GATEWAY_PORT=18789

OPENCLAW_CONFIG_DIR=/home/enver/docker/.openclaw
OPENCLAW_WORKSPACE_DIR=/home/enver/docker/.openclaw/workspace

# GOG Keyring (for encrypted storage)
GOG_KEYRING_PASSWORD=<your-keyring-password>

# Claude Integration (optional)
XDG_CONFIG_HOME=/home/node/.openclaw
CLAUDE_WEB_COOKIE=<your-cookie>
CLAUDE_AI_SESSION_KEY=<your-session-key>
CLAUDE_WEB_SESSION_KEY=<your-session-key>
